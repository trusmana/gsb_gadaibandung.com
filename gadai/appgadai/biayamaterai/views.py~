			###Teddy Uber Penambahan SALDO kas

			 ###Teddy Ciwastra Penambahan SALDO kas
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'PUSAT' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_pusat(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'JAKARTA' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_jakarta(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'SUCI' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_suci(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'DIPATIUKUR' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_du(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'BALUBUR' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_balubur(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'HILIR' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_hilir(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'KOPO' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_kopo(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'CIBIRU' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_cibiru(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'CIPACING' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_cipacing(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'JATINANGOR' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_jatinangor(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'CIMAHI' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_cimahi(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'BUAHBATU' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_bubat(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'MARANATA' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_maranatha(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai ==  'CIREBON' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_cirebon(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'CIUMBULEUIT' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_ciumbeleuit(biaya, request.user)
			if biaya.penambahan_saldo > 0 and biaya.antar_gerai == 'UJUNGBERUNG' and biaya.js_trans == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_penambahan_saldo_kas_ciwastra_uber(biaya, request.user)
			###Teddy Ciwastra Penambahan SALDO kas

			#####pengembalian uk
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='1' :#
				jurnal_biaya_pengembalian_uk_jkt(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='2' :#
				jurnal_biaya_pengembalian_uk_suci(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='3' :#
				jurnal_biaya_pengembalian_uk_du(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='4' :#
				jurnal_biaya_pengembalian_uk_balubur(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='6' :#
				jurnal_biaya_pengembalian_uk_gerlong_hilir(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='7' :#
				jurnal_biaya_pengembalian_uk_kopo(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='8' :#
				jurnal_biaya_pengembalian_uk_cibiru(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='9' :#
				jurnal_biaya_pengembalian_uk_cipacing(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='10' :#
				jurnal_biaya_pengembalian_uk_jatinangor(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='11' :#
				jurnal_biaya_pengembalian_uk_cimahi(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='12' :#
				jurnal_biaya_pengembalian_uk_bubat(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='13' :#
				jurnal_biaya_pengembalian_uk_kordon(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='14' :#
				jurnal_biaya_pengembalian_uk_cihanjuang(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='15' :#
				jurnal_biaya_pengembalian_uk_maranata(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='16' :#
				jurnal_biaya_pengembalian_uk_kircon(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='17' :#
				jurnal_biaya_pengembalian_uk_crbp(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='18' :#
				jurnal_biaya_pengembalian_uk_cmblt(biaya, request.user)
			if biaya.pengembalian_uk > 0  and biaya.gerai.kode_cabang =='20' :#
				jurnal_biaya_pengembalian_uk_ujb(biaya, request.user)
			#####pengembalian

			if biaya.pengembalian_saldo > 0 and biaya.antar_gerai_kembali== 'PUSAT' and biaya.js_trans_kembali == 'BANK' and biaya.gerai.kode_cabang =='1' :#
				jurnal_biaya_pengembalian_saldo_antarbank_jkt(biaya, request.user)
