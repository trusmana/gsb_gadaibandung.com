		jenis = '%s' % ("GL_GL_CABANG"), id_coa = a_foto_copy_debet,
		kredit = 0,debet = D((biaya.foto_copy)),id_product = '4',status_jurnal ='1',tgl_trans =biaya.tanggal,
		id_cabang =biaya.gerai.kode_cabang,id_unit= 300)

