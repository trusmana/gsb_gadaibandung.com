from django.contrib.auth.models import User,Group
from django.core.paginator import Paginator, InvalidPage, EmptyPage
from django.contrib import messages

def cari(request):
	rekening=request.GET['rekening']

	try:
		ag=AkadGadai.objects.get(id=int(rekening))
		return HttpResponseRedirect("/biaya/%s/show/" % ag.id)
	except:
		request.user.message_set.create(message="No rekening tidak ditemukan.")
		return HttpResponseRedirect("/biaya/")

def edit(request,object_id):
	biy=Biaya.objects.get(id=object_id)

	template='biaya/edit.html'
	variable = RequestContext(request,{'biy': biy})
	return render_to_response(template,variable)

def hapus_jurnal(request,object_id):
	tbl = Tbl_Transaksi.objects.get(id=object_id)
	tbl.delete()
	messages.add_message(request, messages.INFO, 'Penghapusan Jurnal Berhasil')
	return HttpResponseRedirect(tbl.get_absolute_url_biaya() )

def biaya_post(request):
	for i in request.POST.getlist('id_pilih'):
		gl = Tbl_Transaksi.objects.get(id=(i))
		gl.status_jurnal = 2
		#gl.jenis = 'GL_GL_CABANG'
		messages.add_message(request, messages.INFO,' Input GL cabang Terposting.')
		gl.save()
	return HttpResponseRedirect('/')

def show(request,object_id):
	ag = Biaya.objects.get(id=object_id)
	sekarang = datetime.datetime.now()
	template = 'biaya/show.html'
	variable = RequestContext(request, {
		'ag': ag})
	return render_to_response(template,variable)

def list(request):
	bea = Biaya.objects.all()
	paginator = Paginator(bea, 50)

	try:
		page = int(request.GET.get('page', '1'))
	except ValueError:
		page = 1
	try:
		bea = paginator.page(page)
	except (EmptyPage, InvalidPage):
		bea = paginator.page(paginator.num_pages)

	template='biaya/index.html'
	variable = RequestContext(request,{'bea': bea})
	return render_to_response(template,variable)

def add(request,object_id):
	sekarang = datetime.date.today()
	c = object_id
	bea = Tbl_Transaksi.objects.filter(tgl_trans=sekarang).filter(id_cabang=object_id).filter(status_jurnal = 1L).\
	filter(jenis__in=( u'GL_GL_CABANG',u'GL_GL_PUSAT_UK',u'GL_GL_PUSAT',u'GL_GL_BIAYA_GERAI','GL_GL_CABANG_PENGEMBALIAN','GL_GL_PENAMBAHAN_GERAI','GL_GL_PENAMBAHAN_BANK','GL_GL_PENAMBAHAN_KAS','GL_GL_PENGELUARAN_KAS_PUSAT',
		'GL_GL_PENGEMBALIAN_GERAI','GL_GL_PENGEMBALIAN_PUSAT','GL_GL_PENAMBAHAN_SALDO','GL_GL_PENGELUARAN_BANK','GL_GL_PENGELUARAN_BANK_PUSAT','GL_GL_PENGELUARAN_KAS',\
		'GL_GL_PENGEMBALIAN_UK','GL_GL_CABANG_UK'))
		
	user = request.user
	if request.method == "POST":
		form = BiayaForm(request.POST)
		if form.is_valid():
			gerai = form.cleaned_data['gerai']
			tanggal = form.cleaned_data['tanggal']

			listrik = form.cleaned_data['listrik']
			ket_listrik = form.cleaned_data['ket_listrik']
			pdam  = form.cleaned_data['pdam']
			ket_pdam = form.cleaned_data['ket_pdam']
			telpon = form.cleaned_data['telpon']
			ket_telpon = form.cleaned_data['ket_telpon']

			majalah = form.cleaned_data['majalah']
			ket_majalah = form.cleaned_data['ket_majalah']
			#iuran_keamanan = form.cleaned_data['iuran_keamanan']
			#ket_iuran_keamanan = form.cleaned_data['ket_iuran_keamanan']
			#iuran_kebersihan = form.cleaned_data['iuran_kebersihan']
			#ket_iuran_kebersihan = form.cleaned_data['ket_iuran_kebersihan']

			#air_minum = form.cleaned_data['air_minum']
			#ket_air_minum = form.cleaned_data['ket_air_minum']

			penambahan_saldo = form.cleaned_data['penambahan_saldo']
			ket_penambahan_saldo = form.cleaned_data['ket_penambahan_saldo']
			pengembalian_saldo = form.cleaned_data['pengembalian_saldo']
			ket_pengembalian_saldo = form.cleaned_data['ket_pengembalian_saldo']
			penambahan_uk = form.cleaned_data['penambahan_uk']
			ket_penambahan_uk = form.cleaned_data['ket_penambahan_uk']
			pengembalian_uk = form.cleaned_data['pengembalian_uk']
			ket_pengembalian_uk = form.cleaned_data['ket_pengembalian_uk']

			palkir = form.cleaned_data['palkir']
			ket_palkir = form.cleaned_data['ket_palkir']
			bbm = form.cleaned_data['bbm']
			ket_bbm = form.cleaned_data['ket_bbm']
			#pulsa = form.cleaned_data['pulsa']
			#ket_pulsa = form.cleaned_data['ket_pulsa']
			materai = form.cleaned_data['materai']
			ket_materai = form.cleaned_data['ket_materai']
			pemb_lingkungan = form.cleaned_data['pemb_lingkungan']
			ket_pemb_lingkungan = form.cleaned_data['ket_pemb_lingkungan']
			sumbangan = form.cleaned_data['sumbangan']
			ket_sumbangan = form.cleaned_data['ket_sumbangan']
			perlengkapan = form.cleaned_data['perlengkapan']
			ket_perlengkapan = form.cleaned_data['ket_perlengkapan']
			konsumsi = form.cleaned_data['konsumsi']
			ket_konsumsi = form.cleaned_data['ket_konsumsi']
			foto_copy  = form.cleaned_data['foto_copy']
			ket_foto_copy = form.cleaned_data['ket_foto_copy']
			lain_lain = form.cleaned_data['lain_lain']
			ket_lain_lain = form.cleaned_data['ket_lain_lain']
			nilai_lain_lain = form.cleaned_data['nilai_lain_lain']
			
			js_trans = form.cleaned_data['js_trans']
			antar_gerai = form.cleaned_data['antar_gerai']
			pendapatan_lain = form.cleaned_data['pendapatan_lain']
			ket_pendapatan_lain = form.cleaned_data['ket_pendapatan_lain']
			js_trans_kembali = form.cleaned_data['js_trans_kembali']
			antar_gerai_kembali = form.cleaned_data['antar_gerai_kembali']
			
			jenis_transaksi_listrik	= form.cleaned_data['jenis_transaksi_listrik']
			jenis_transaksi_pdam	= form.cleaned_data['jenis_transaksi_pdam']
			jenis_transaksi_telepon	= form.cleaned_data['jenis_transaksi_telepon']
			jenis_transaksi_foto_copy	= form.cleaned_data['jenis_transaksi_foto_copy']
			jenis_transaksi_majalah	= form.cleaned_data['jenis_transaksi_majalah']
			#jenis_transaksi_iuran_keamanan	= form.cleaned_data['jenis_transaksi_iuran_keamanan']
			#jenis_transaksi_iuran_kebersihan	= form.cleaned_data['jenis_transaksi_iuran_kebersihan']
			#jenis_transaksi_air_minum	= form.cleaned_data['jenis_transaksi_air_minum']
			jenis_transaksi_palkir	= form.cleaned_data['jenis_transaksi_palkir']
			jenis_transaksi_bbm	= form.cleaned_data['jenis_transaksi_bbm']
			#jenis_transaksi_pulsa	= form.cleaned_data['jenis_transaksi_pulsa']
			jenis_transaksi_materai	= form.cleaned_data['jenis_transaksi_materai']
			jenis_transaksi_pemb_lingkungan	= form.cleaned_data['jenis_transaksi_pemb_lingkungan']
			jenis_transaksi_sumbangan	= form.cleaned_data['jenis_transaksi_sumbangan']
			jenis_transaksi_perlengkapan	= form.cleaned_data['jenis_transaksi_perlengkapan']
			jenis_transaksi_konsumsi	= form.cleaned_data['jenis_transaksi_konsumsi']
			jenis_transaksi_nilai_lain_lain= form.cleaned_data['jenis_transaksi_nilai_lain_lain']

			biaya = Biaya(gerai = gerai,tanggal = tanggal,listrik = listrik,ket_listrik = ket_listrik,pdam  = pdam,ket_pdam = ket_pdam,telpon = telpon,
				ket_telpon = ket_telpon,majalah = majalah,ket_majalah = ket_majalah,
				#air_minum = air_minum,ket_air_minum = ket_air_minum,
				penambahan_saldo = penambahan_saldo,ket_penambahan_saldo = ket_penambahan_saldo,pengembalian_saldo = pengembalian_saldo,
				ket_pengembalian_saldo = ket_pengembalian_saldo,penambahan_uk = penambahan_uk,ket_penambahan_uk = ket_penambahan_uk,
				pengembalian_uk = pengembalian_uk,ket_pengembalian_uk = ket_pengembalian_uk,palkir = palkir,
				ket_palkir = ket_palkir,bbm = bbm,ket_bbm = ket_bbm,
				materai = materai,ket_materai = ket_materai,
				foto_copy  = foto_copy,ket_foto_copy = ket_foto_copy,pemb_lingkungan  = pemb_lingkungan,ket_pemb_lingkungan = ket_pemb_lingkungan,
				sumbangan  = sumbangan,ket_sumbangan = ket_sumbangan,perlengkapan  = perlengkapan,ket_perlengkapan = ket_perlengkapan,
				konsumsi  = konsumsi,ket_konsumsi = ket_konsumsi,lain_lain = lain_lain,ket_lain_lain = ket_lain_lain,nilai_lain_lain = nilai_lain_lain,
				ket_pendapatan_lain=ket_pendapatan_lain,pendapatan_lain=pendapatan_lain,js_trans_kembali=js_trans_kembali,
				antar_gerai=antar_gerai,js_trans = js_trans,antar_gerai_kembali = antar_gerai_kembali,
				jenis_transaksi_listrik	= jenis_transaksi_listrik,jenis_transaksi_pdam	= jenis_transaksi_pdam,jenis_transaksi_telepon	= jenis_transaksi_telepon,jenis_transaksi_foto_copy	= jenis_transaksi_foto_copy,
				jenis_transaksi_majalah	= jenis_transaksi_majalah,
				#jenis_transaksi_air_minum	= jenis_transaksi_air_minum,
				jenis_transaksi_palkir	= jenis_transaksi_palkir,jenis_transaksi_bbm	= jenis_transaksi_bbm,
				jenis_transaksi_materai	= jenis_transaksi_materai,jenis_transaksi_pemb_lingkungan	= jenis_transaksi_pemb_lingkungan,jenis_transaksi_sumbangan	= jenis_transaksi_sumbangan,
				jenis_transaksi_konsumsi	= jenis_transaksi_konsumsi,jenis_transaksi_perlengkapan	= jenis_transaksi_perlengkapan,jenis_transaksi_nilai_lain_lain= jenis_transaksi_nilai_lain_lain)
			biaya.save()
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '2': #
				jurnal_biaya_listrik_panjar(biaya, request.user)
			####LISTRIK PER GERAI
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='1':
				jurnal_biaya_listrik_jkt(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='2':
				jurnal_biaya_listrik_suci(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='3':
				jurnal_biaya_listrik_du(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='4':
				jurnal_biaya_listrik_balubur(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='6':
				jurnal_biaya_listrik_hilir(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='7':
				jurnal_biaya_listrik_kopo(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='8':
				jurnal_biaya_listrik_cibiru(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='9':
				jurnal_biaya_listrik_cipacing(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='10':
				jurnal_biaya_listrik_jatinangor(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='11':
				jurnal_biaya_listrik_cimahi(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='12':
				jurnal_biaya_listrik_bubat(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='13':
				jurnal_biaya_listrik_kordon(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='14':
				jurnal_biaya_listrik_cihanjuang(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='15':
				jurnal_biaya_listrik_maranata(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='16':
				jurnal_biaya_listrik_kircon(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='17':
				jurnal_biaya_listrik_cirebon_perjuangan(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='19':
				jurnal_biaya_listrik_cimuleuit(biaya, request.user)
			if biaya.listrik > 0 and biaya.jenis_transaksi_listrik == '1' and biaya.gerai.kode_cabang =='20':
				jurnal_biaya_listrik_uber(biaya, request.user)
			####LISTRIK PER GERAI
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '2':#
				jurnal_biaya_bbm_panjar(biaya, request.user)

			####bbm per gerai
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang == '1':
				jurnal_biaya_bbm_jkt(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='2':
				jurnal_biaya_bbm_suci(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='3':
				jurnal_biaya_bbm_du(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='4':
				jurnal_biaya_bbm_balubur(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='6':
				jurnal_biaya_bbm_hilir(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='7':
				jurnal_biaya_bbm_kopo(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='8':
				jurnal_biaya_bbm_cibiru(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='9':
				jurnal_biaya_bbm_cipacing(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='10':
				jurnal_biaya_bbm_jatinangor(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='11':
				jurnal_biaya_bbm_cimahi(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='12':
				jurnal_biaya_bbm_bubat(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='13':
				jurnal_biaya_bbm_kordon(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='14':
				jurnal_biaya_bbm_cihanjuang(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='15':
				jurnal_biaya_bbm_maranata(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='16':
				jurnal_biaya_bbm_kircon(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='17':
				jurnal_biaya_bbm_cirebon_perjuangan(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='19':
				jurnal_biaya_bbm_cimuleuit(biaya, request.user)
			if biaya.bbm > 0 and biaya.jenis_transaksi_bbm == '1' and biaya.gerai.kode_cabang =='20':
				jurnal_biaya_bbm_uber(biaya, request.user)

			####bbm per gerai

			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '2':#
				jurnal_biaya_foto_copy_panjar(biaya, request.user)

			####FOTO COPY per gerai
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang == '1':
				jurnal_biaya_foto_copy_jkt(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='2':
				jurnal_biaya_foto_copy_suci(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='3':
				jurnal_biaya_foto_copy_du(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='4':
				jurnal_biaya_foto_copy_balubur(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='6':
				jurnal_biaya_foto_copy_hilir(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='7':
				jurnal_biaya_foto_copy_kopo(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='8':
				jurnal_biaya_foto_copy_cibiru(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='9':
				jurnal_biaya_foto_copy_cipacing(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='10':
				jurnal_biaya_foto_copy_jatinangor(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='11':
				jurnal_biaya_foto_copy_cimahi(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='12':
				jurnal_biaya_foto_copy_bubat(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='13':
				jurnal_biaya_foto_copy_kordon(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='14':
				jurnal_biaya_foto_copy_cihanjuang(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='15':
				jurnal_biaya_foto_copy_maranata(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='16':
				jurnal_biaya_foto_copy_kircon(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='17':
				jurnal_biaya_foto_copy_cirebon_perjuangan(biaya, request.user)
			if biaya.foto_copy > 0 and biaya.jenis_transaksi_foto_copy == '1' and biaya.gerai.kode_cabang =='19':
				jurnal_biaya_foto_copy_cimuleuit(biaya, request.user)
