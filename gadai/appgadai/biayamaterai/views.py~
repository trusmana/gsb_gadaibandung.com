		jenis = '%s' % ("GL_GL_CABANG"), id_coa = a_iuran_keamanan_kredit,
		debet = 0,kredit = D((biaya.iuran_keamanan)),id_product = '4',status_jurnal ='1',tgl_trans =biaya.tanggal,
		id_cabang =biaya.gerai.kode_cabang,id_unit= 300)

