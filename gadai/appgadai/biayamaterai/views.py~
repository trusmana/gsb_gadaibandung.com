		jenis = '%s' % ("GL_GL_PENAMBAHAN_KAS"), id_coa = a_penambahan_saldo_debet,
		kredit = 0,debet = D((biaya.penambahan_saldo)),id_product = '4',status_jurnal ='1',tgl_trans =biaya.tanggal,
		id_cabang =biaya.gerai.kode_cabang,id_unit= 300)

