				jurnal_biaya_pengembalian_saldo_kas_ciwastra_bubat(biaya, request.user)
			if biaya.pengembalian_saldo > 0 and biaya.antar_gerai_kembali == 'MARANATA' and biaya.js_trans_kembali == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_pengembalian_saldo_kas_ciwastra_maranatha(biaya, request.user)
			if biaya.pengembalian_saldo > 0 and biaya.antar_gerai_kembali == 'CIREBON' and biaya.js_trans_kembali == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_pengembalian_saldo_kas_ciwastra_cirebon(biaya, request.user)
			if biaya.pengembalian_saldo > 0 and biaya.antar_gerai_kembali == 'CIUMBULEUIT' and biaya.js_trans_kembali == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_pengembalian_saldo_kas_ciwastra_ciumbeleuit(biaya, request.user)
			if biaya.pengembalian_saldo > 0 and biaya.antar_gerai_kembali == 'UJUNGBERUNG' and biaya.js_trans_kembali == 'KAS' and biaya.gerai.kode_cabang =='21' :#
				jurnal_biaya_pengembalian_saldo_kas_ciwastra_uber(biaya, request.user)

			messages.add_message(request, messages.INFO,"JURNAL TELAH TERSIMPAN")
			return HttpResponseRedirect('/biaya/%s/add/' % (object_id))
		else:
			form  = BiayaForm()
			form.fields['gerai'].queryset = Tbl_Cabang.objects.filter(kode_cabang=object_id)
		variables = RequestContext(request, {'form': form,'bea':bea,'c':object_id,'total_kredit': sum([p.kredit for p in bea]),'total_debet': sum([p.kredit for p in bea])})
		return HttpResponseRedirect('/biaya/%s/add/' % (object_id))
	else:
		form  = BiayaForm()
		form.fields['gerai'].queryset = Tbl_Cabang.objects.filter(kode_cabang=object_id)
	variables = RequestContext(request, {'form': form,'bea':bea,'c':object_id,'total_kredit': sum([p.kredit for p in bea]),'total_debet': sum([p.kredit for p in bea])})
	return render_to_response('biaya/addbiaya.html', variables)


###Teddy jakarta Penambahan SALDO bank
def jurnal_biaya_penambahan_saldo_bank_jakarta(biaya, user):
	D = decimal.Decimal
	a_penambahan_saldo_debet = get_object_or_404(Tbl_Akun, id=7L)
	a_penambahan_saldo_kredit = get_object_or_404(Tbl_Akun, id=133L)
	jurnal = Jurnal.objects.create(
		diskripsi= 'Setoran Dari Gerai %s melalui %s ' % (biaya.antar_gerai,biaya.js_trans),
		tgl_trans = biaya.tanggal,cu = user, mu = user,nobukti=biaya.ket_penambahan_saldo)

	jurnal.tbl_transaksi_set.create(
		jenis = '%s' % ("GL_GL_PENAMBAHAN_BANK"), id_coa = a_penambahan_saldo_debet,
		kredit = 0,debet = D((biaya.penambahan_saldo)),id_product = '4',status_jurnal ='1',tgl_trans =biaya.tanggal,
		id_cabang =biaya.gerai.kode_cabang,id_unit= 300)

	jurnal.tbl_transaksi_set.create(
		jenis = '%s' % ("GL_GL_PENAMBAHAN_BANK"), id_coa = a_penambahan_saldo_kredit,
		debet = 0,kredit = D((biaya.penambahan_saldo)),id_product = '4',status_jurnal ='1',tgl_trans =biaya.tanggal,
		id_cabang =biaya.gerai.kode_cabang,id_unit= 300)
###Teddy jakarta Penambahan SALDO bank

