####Jurnal dari Jakarta ke Gerai dan Pusat
def jurnal_biaya_pengembalian_saldo_antarbank_jkt(biaya, user):
	D = decimal.Decimal
	a_pengembalian_saldo_debet = get_object_or_404(Tbl_Akun, id=206L)
